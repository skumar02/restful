package com.java.jersey.messenger.data;

import java.util.HashMap;
import java.util.Map;

import com.java.jersey.messenger.model.Message;

public class MessengerData {
	
	private Map<Long, Message> messageMap = new HashMap<>();

	public Map<Long, Message> getMessageMap() {
		return messageMap;
	}

	public void setMessageMap(Map<Long, Message> messageMap) {
		this.messageMap = messageMap;
	}

}
